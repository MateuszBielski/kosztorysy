<tr>
	<td>
		{% block letter %}{% endblock %}
	</td>
</tr>
{% for row in circulations %}
	<tr>
		{% block cols %}
			<td>{{ row.name }}</td>
			<td>{{ row.value }}</td>
			<td>{{ row.unit }}</td>
			{# {% if row.groupNumber is defined %}
			<td>{{ row.groupNumber }}</td>
			{% endif %} #}
			{% set price = row.getPriceDivBy100() %}
			{% if price >= 0.0 %}
			<td>{{ price }}</td>
			{% endif %}
		{% endblock %}
	</tr>
{% endfor %}
